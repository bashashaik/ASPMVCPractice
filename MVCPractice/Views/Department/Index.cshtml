@using MVCPractice.Models
@model IEnumerable<Department>

@{
    ViewBag.Title = "Index";
}

<h2>Departments</h2>
<ul>
    @foreach (Department objDepartment in @Model)
    {
        <li>
            @Html.ActionLink(@objDepartment.Name, "Index", "Employee", new { intDepartmentId = @objDepartment.Id }, null)
        </li>
    }
</ul>
